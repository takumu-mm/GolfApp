<header>
  <div class = 'header-top'>
    <div class = 'header-btn'>
      <%= link_to 'ログイン' , "#", class: 'btn btn-primary' %>
      <%= link_to '新規登録' , "#", class: 'btn btn-primary' %>
    </div>
  </div>
  <nav class='navbar navbar-expand-lg navigation'>
    <button class='navbar-toggler' data-bs-toggle='collapse' data-bs-target='#navbarSupportedContent'
      aria-controls='navbarSupportedContent' aria-expanded='false' aria-label='Toggle navigation'>
      <span class='navbar-toggler-icon'></span>
    </button>
    <div class='collapse navbar-collapse' id='navbarSupportedContent'>
      <ul class='navbar-nav ms-auto main-nav align-items-center'>
        <li class='nav-item dropdown '>
            <%= link_to t('header.basic'), '#', class: 'nav-link', 'data-bs-toggle' => 'dropdown', 'aria-haspopup' => 'true', 'aria-expanded' => 'false' %>
          <div class='dropdown-menu dropdown-menu-end'>
            <%= link_to t('header.address') , "#", class: 'dropdown-item' %>
            <%= link_to t('header.grip') , "#", class: 'dropdown-item' %>
            <%= link_to t('header.basic_swing') , "#", class: 'dropdown-item' %>
          </div>
        </li>
        <li class='nav-item dropdown '>
            <%= link_to t('header.club'), '#', class: 'nav-link', 'data-bs-toggle' => 'dropdown', 'aria-haspopup' => 'true', 'aria-expanded' => 'false' %>
          <div class='dropdown-menu dropdown-menu-end'>
            <%= link_to t('header.driver') , "#", class: 'dropdown-item' %>
            <%= link_to t('header.iron') , "#", class: 'dropdown-item' %>
            <%= link_to t('header.wood') , "#", class: 'dropdown-item' %>
            <%= link_to t('header.utility') , "#", class: 'dropdown-item' %>
            <%= link_to t('header.putter') , "#", class: 'dropdown-item' %>
          </div>
        </li>
        <li class='nav-item dropdown '>
            <%= link_to t('header.environment'), '#', class: 'nav-link', 'data-bs-toggle' => 'dropdown', 'aria-haspopup' => 'true', 'aria-expanded' => 'false' %>
          <div class='dropdown-menu dropdown-menu-end'>
            <%= link_to t('header.bunker') , "#", class: 'dropdown-item' %>
            <%= link_to t('header.rough') , "#", class: 'dropdown-item' %>
            <%= link_to t('header.strongwind') , "#", class: 'dropdown-item' %>
            <%= link_to t('header.rain') , "#", class: 'dropdown-item' %>
          </div>
        </li>
        <li class='nav-item dropdown '>
            <%= link_to t('header.green_around'), '#', class: 'nav-link', 'data-bs-toggle' => 'dropdown', 'aria-haspopup' => 'true', 'aria-expanded' => 'false' %>
          <div class='dropdown-menu dropdown-menu-end'>
            <%= link_to t('header.approach') , "#", class: 'dropdown-item' %>
            <%= link_to t('header.lob_shot') , "#", class: 'dropdown-item' %>
            <%= link_to t('header.ground_ball') , "#", class: 'dropdown-item' %>
          </div>
        </li>
        </li>
        <li class='nav-item dropdown '>
            <%= link_to t('header.shot_miss'), '#', class: 'nav-link', 'data-bs-toggle' => 'dropdown', 'aria-haspopup' => 'true', 'aria-expanded' => 'false' %>
          <div class='dropdown-menu dropdown-menu-end'>
            <%= link_to t('header.hook') , "#", class: 'dropdown-item' %>
            <%= link_to t('header.slice') , "#", class: 'dropdown-item' %>
            <%= link_to t('header.duff') , "#", class: 'dropdown-item' %>
            <%= link_to t('header.top') , "#", class: 'dropdown-item' %>
            <%= link_to t('header.tempura') , "#", class: 'dropdown-item' %>
          </div>
        </li>
        <li class='nav-item dropdown '>
            <%= link_to t('header.mastering_items'), '#', class: 'nav-link', 'data-bs-toggle' => 'dropdown', 'aria-haspopup' => 'true', 'aria-expanded' => 'false' %>
          <div class='dropdown-menu dropdown-menu-end'>
            <%= link_to t('header.flight_distance') , "#", class: 'dropdown-item' %>
            <%= link_to t('header.stability') , "#", class: 'dropdown-item' %>
            <%= link_to t('header.management') , "#", class: 'dropdown-item' %>
          </div>
        </li>
        <li class='nav-item dropdown '>
            <%= link_to t('header.target'), '#', class: 'nav-link', 'data-bs-toggle' => 'dropdown', 'aria-haspopup' => 'true', 'aria-expanded' => 'false' %>
          <div class='dropdown-menu dropdown-menu-end'>
            <%= link_to t('header.100_or_less') , "#", class: 'dropdown-item' %>
            <%= link_to t('header.90_or_less') , "#", class: 'dropdown-item' %>
            <%= link_to t('header.80_or_less') , "#", class: 'dropdown-item' %>
          </div>
        </li>
        <li class='nav-item dropdown'>
            <%= link_to t('header.diagnosis'), '#', class: 'nav-link' %>
        </li>
        <li class='nav-item dropdown'>
            <%= link_to t('header.recommended'), '#', class: 'nav-link' %>
        </li>
        <li class='nav-item dropdown '>
          <%= link_to('#', class: 'nav-link', data: { bs_toggle: 'dropdown' }, aria: { haspopup: 'true',  expanded: 'false' }, id: 'header-profile' ) do %>
            <i class="bi bi-house-door-fill"></i>
          <% end %>
          <div class='dropdown-menu dropdown-menu-end'>
            <div class='dropdown-item'><%= "ユーザー名" %></div>
            <div class='dropdown-divider'></div>
            <%= link_to t('header.profile'), '#', class: 'dropdown-item' %>
            <%= link_to t('header.logout'), "#", class: 'dropdown-item', data: { turbo_method: :delete } %>
          </div>
        </li>
      </ul>
    </div>
  </nav>
</header>